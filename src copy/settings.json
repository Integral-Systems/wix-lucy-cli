    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.1.8",
    "madge": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@typescript-eslint/utils": "^8.35.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@wix/cli": "^1.1.101",
    "@wix/eslint-plugin-cli": "^1.0.2",
    "cypress": "^14.5.1",
